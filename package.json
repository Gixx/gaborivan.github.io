{
  "name": "gabor-ivan-blog",
  "version": "2022.1.0",
  "description": "The open sourced content of the gaborivan.de site generated by Jekyll static site generator.",
  "author": "Gabor Ivan (https://www.gaborivan.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gixx/gaborivan.github.io"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "esprima": "^4.0.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "npm-run-all":"^4.1.5"
  },
  "scripts": {
    "develop": "bundle exec jekyll clean && run-p develop:{webpack,jekyll}",
    "develop:jekyll": "sleep 5 && bundle exec jekyll serve --host 0.0.0.0 -w --drafts --force_poll --trace",
    "develop:webpack": "webpack --mode development --watch",
    "build": "run-s build:webpack build:jekyll",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "build:webpack": "webpack --mode production"
  }
}
